{
  "private": true,
  "name": "@ionic/angular",
  "version": "4.0.0",
  "description": "Angular specific wrappers for @ionic/core",
  "keywords": [
    "ionic",
    "framework",
    "mobile",
    "app",
    "hybrid",
    "webapp",
    "cordova",
    "progressive web app",
    "pwa"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/ionic.git"
  },
