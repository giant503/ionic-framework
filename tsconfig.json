{
    "version": "1.5.0",
    "compilerOptions": {
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "outDir": "dist/js/cjs"
    },
    "fileGlobs": [
        "./ionic/**/*.ts",
        "!./node_modules/**",
        "!./scripts/**",
        "!./dist/**",
        "!./tmp/**"
    ]
}
